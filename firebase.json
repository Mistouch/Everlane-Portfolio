{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/Men_Tees/**",
        "destination": "/Men_Tees/**"
      },
      {
        "source": "/Men_Denim_Pant/**",
        "destination": "/Men_Denim_Pant/**"
      },
      {
        "source": "/Men_Denim_Jacket/**",
        "destination": "/Men_Denim_Jacket/**"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [{
      "source": "/**/*.@(jpg|jpeg|gif|png)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=7200"
      }]
    }]
  }
}