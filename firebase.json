{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/Men_Tees/**",
        "destination": "/Men_Tees/**"
      },
      {
        "source": "/Men_Denim_Pant/**",
        "destination": "/Men_Denim_Pant/**"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
